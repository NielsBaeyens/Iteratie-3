package main;

import java.util.Arrays;
import java.util.List;

import domein.Spel;
import domein.Speler;
import ui.ColorettoApplicatie;

public class StartUp {

	public static void main(String[] args) {
		List<Speler> spelers = Arrays.asList(new Speler("Tony Starck"), new Speler("Peter Parker"),
				new Speler("Clark Kent"), new Speler("Bruce Wayne"));
		Spel spel = new Spel(spelers);
		ColorettoApplicatie app = new ColorettoApplicatie(spel);
		
	}

}
